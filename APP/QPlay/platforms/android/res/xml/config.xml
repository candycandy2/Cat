<?xml version='1.0' encoding='utf-8'?>
<widget id="com.BenQ.QPlay" version="0.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <feature name="QLoginPlugin">
        <param name="android-package" value="org.apache.cordova.qlogin.QLoginPlugin" />
    </feature>
    <feature name="Device">
        <param name="android-package" value="org.apache.cordova.device.Device" />
    </feature>
    <feature name="QSecurityPlugin">
        <param name="android-package" value="org.apache.cordova.qsecurity.QSecurityPlugin" />
        <param name="onload" value="true" />
    </feature>
    <name>QPlay</name>
    <description>
        BenQ Enterprise application framework
    </description>
    <author email="alan.tu@benq.com" href="http://benq.com">
        BenQ ITS
    </author>
    <content src="index.html" />
    <serverUrl href="http://qplay.benq.com/qplayApi/public/index.php/qplayauth_register" />
    <preference name="loglevel" value="DEBUG" />
    <preference name="StatusBarOverlaysWebView" value="true" />
    <preference name="StatusBarBackgroundColor" value="#FF0000" />
</widget>
